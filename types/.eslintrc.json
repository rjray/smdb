{
  "root": true,
  "env": {
    "jest/globals": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:promise/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": [
    "@typescript-eslint",
    "promise",
    "jest"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "warn"
  },
  "parser": "@typescript-eslint/parser"
}
